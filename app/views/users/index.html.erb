<% provide(:title, 'All users') %>

<h1 class="text-2xl font-semibold text-gray-800 dark:text-white my-4 text-center">Users</h1>

<%= render 'search_form' %>

<tubro-frame id="search">
    <ul role="list" class="divide-y divide-gray-100 w-full md:w-auto">
        <%= render @users %>
    </ul>
</turbo-frame>
<%= will_paginate @users %>
